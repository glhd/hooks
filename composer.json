{
  "name": "glhd/hooks",
  "description": "",
  "keywords": [
    "laravel"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://www.cmorrell.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10|11.x-dev|dev-master",
    "spatie/laravel-package-tools": "^1.15"
  },
  "require-dev": {
    "orchestra/testbench": "^8|9.x-dev|10.x-dev|dev-master",
    "friendsofphp/php-cs-fixer": "^3.34",
    "phpunit/phpunit": "^10.3"
  },
  "autoload": {
    "psr-4": {
      "Glhd\\Hooks\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Glhd\\Hooks\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix-style": "vendor/bin/php-cs-fixer fix",
    "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Glhd\\Hooks\\Support\\HooksServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
